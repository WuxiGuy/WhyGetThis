{
	"manifest_version": 3,
	"name": "Why Get This",
	"version": "1.0",
	"description": "Track and understand your browsing habits.",
	"permissions": ["storage", "activeTab", "tabs"],
	"background": {
	  "service_worker": "scripts/background.js"
	},
	"action": {
	  "default_popup": "pages/popup.html"
	},
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["scripts/content.js"]
	  }
	],
	"host_permissions": [
	  "<all_urls>"
	]
}
  